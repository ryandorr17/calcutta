<div id="wrapper">
  <h1>All Site Users</h1>

  <table class="table table-striped table-condensed">
    <thead>
      <tr>
        <th>Name</th>
        <th>email</th>
        <th>Profile Page</th>

      </tr>
    </thead>

    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= link_to 'profile', user %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
